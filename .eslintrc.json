{
    "env": {
        "node": true,
        "es6": true,
        "commonjs": true,
        "mocha": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {"ecmaVersion": 2017, "sourceType": "module"},
    "rules": {
        "no-console": "off",
        "prettier/prettier": "error"
    }
}
